{
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@20.x"
    },
    "test-api.js": {
      "runtime": "@vercel/node@20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/test",
      "destination": "/test-api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
